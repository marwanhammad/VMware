<routing>
	<enabled>true</enabled>
	<routingGlobalConfig>
		<routerId>VSERouterID</routerId>
		<ecmp>false</ecmp>
		<logging>
			<enable>false</enable>
			<logLevel>info</logLevel>
		</logging>
	</routingGlobalConfig>
	<staticRouting>
		<defaultRoute>
			<vnic>0</vnic>
			<mtu>1500</mtu>
			<gatewayAddress>100.64.102.254</gatewayAddress>
			<adminDistance>1</adminDistance>
		</defaultRoute>
		<staticRoutes/>
	</staticRouting>
	<ospf>
		<enabled>true</enabled>
		<ospfAreas>
			<ospfArea>
				<areaId>20</areaId>
				<type>normal</type>
				<authentication>
					<type>none</type>
				</authentication>
			</ospfArea>
		</ospfAreas>
		<ospfInterfaces>
			<ospfInterface>
				<vnic>0</vnic>
				<areaId>20</areaId>
				<helloInterval>10</helloInterval>
				<deadInterval>40</deadInterval>
				<priority>128</priority>
				<cost>1</cost>
				<mtuIgnore>false</mtuIgnore>
			</ospfInterface>
		</ospfInterfaces>
		<redistribution>
			<enabled>true</enabled>
			<rules>
				<rule>
					<id>0</id>
					<from>
						<ospf>false</ospf>
						<bgp>false</bgp>
						<static>false</static>
						<connected>false</connected>
					</from>
					<action>permit</action>
				</rule>
			</rules>
		</redistribution>
		<gracefulRestart>true</gracefulRestart>
		<defaultOriginate>false</defaultOriginate>
	</ospf>
</routing>